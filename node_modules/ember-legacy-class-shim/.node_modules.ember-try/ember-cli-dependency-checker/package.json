{
  "name": "ember-cli-dependency-checker",
  "version": "2.0.1",
  "description": "Ember CLI addon for detecting missing npm and bower dependencies before executing ember commands",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "node tests/runner",
    "test:debug": "node debug tests/runner"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quaertym/ember-cli-dependency-checker.git"
  },
  "bugs": {
    "url": "https://github.com/quaertym/ember-cli-dependency-checker/issues"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "author": "Emre Unal",
  "license": "MIT",
  "readmeFile": "README.md",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "chai": "^3.5.0",
    "ember-cli": "^2.13.1",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-eslint": "^3.0.2",
    "ember-cli-htmlbars": "^1.0.10",
    "ember-cli-htmlbars-inline-precompile": "^0.4.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-test-loader": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.1.0",
    "eslint-plugin-chai": "0.0.1",
    "eslint-plugin-mocha": "^4.8.0",
    "glob": "^7.1.1",
    "loader.js": "^4.0.10",
    "mocha": "^3.3.0",
    "mocha-eslint": "^3.0.1"
  },
  "keywords": [
    "ember-addon",
    "ember-cli",
    "ember",
    "node",
    "npm",
    "bower",
    "dependency",
    "check"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "is-git-url": "^1.0.0",
    "semver": "^5.3.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
